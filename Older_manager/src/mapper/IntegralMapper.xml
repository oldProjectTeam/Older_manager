      </if>
    </trim>
  </insert>
  <select id="countByExample" parameterType="com.older.manager.bean.IntegralExample" resultType="java.lang.Integer" >
    select count(*) from integral
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
  </select>
